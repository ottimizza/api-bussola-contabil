extends type Kpi {
	id: BigInteger!
	kpiAlias: String!
	company: CompanyFilter
	title: String!
	subtitle: String
	description: String
	graphType: Short!
	columnX0Label: String!
	label: String
	label2: String
	label3: String
	label4: String
	visible: Boolean
	kpiDetail: [KpiDetail]
}

extends input KpiFilter {
	id: BigInteger
	kpiAlias: String
	company: CompanyFilter
	title: String
	subtitle: String
	description: String
	graphType: Short
	columnX0Label: String
	label: String
	label2: String
	label3: String
	label4: String
	visible: Boolean
	kpiDetail: [KpiDetail]
}

extends type Query {
	findKpi(filter: Kpifilter): [Kpi]
}

extends type Mutation {
	createKpi(companyId: BigInteger, company: Company, kpiAlias: String, title: String, subtitle: String, description: String, graphType: Short, columnX0Label: String, label: String, label2: String, label3: String, label4: String, visible: Boolean): Kpi
	createKpi(companyId: BigInteger, kpi: Kpi): Kpi
